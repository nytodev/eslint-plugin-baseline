{
  "name": "@nytodev/eslint-plugin-baseline",
  "version": "1.0.0",
  "description": "ESLint baseline plugin - Ignore existing errors and only report new ones (PHPStan-like)",
  "main": "src/index.js",
  "types": "src/index.d.ts",
  "bin": {
    "eslint-baseline": "bin/cli.js"
  },
  "exports": {
    ".": {
      "types": "./src/index.d.ts",
      "default": "./src/index.js"
    },
    "./formatter": "./src/formatter.js"
  },
  "files": [
    "src",
    "bin"
  ],
  "scripts": {
    "test": "node --test test/*.test.js",
    "lint": "eslint src bin"
  },
  "keywords": [
    "eslint",
    "eslintplugin",
    "eslint-plugin",
    "baseline",
    "phpstan",
    "legacy",
    "ignore",
    "suppress"
  ],
  "author": "",
  "license": "MIT",
  "peerDependencies": {
    "eslint": ">=8.0.0"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "repository": {
    "type": "git",
    "url": ""
  },
  "bugs": {
    "url": ""
  },
  "homepage": ""
}
